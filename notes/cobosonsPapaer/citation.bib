@article{moth,
	Abstract = {We extend the well-known Cooper's problem beyond one pair and study how this dilute limit is connected to the many-pair BCS configuration. We show that, all over from the dilute to the dense regime of Cooper pairs, Pauli blocking induces the same "moth-eaten effect" as the one existing for composite boson excitons. This effect makes the average pair binding energy decrease linearly with pair number, bringing it, in the standard BCS configuration, to half the single-pair value. This proves that, at odds with common understanding, the BCS gap is far larger than the broken pair energy. The increase comes from a missed Pauli blocking between broken and unbroken pairs. Possible link between our result and the BEC-BCS crossover is also discussed.},
	 author = {Pogosov, Walter V. and Combescot, Monique  },
	Title = {"Moth-eaten effect" driven by Pauli blocking, revealed for Cooper pairs},
	year = {2010},
	journal={JETP Letters},
	Volume={92},
	pages={534}}


